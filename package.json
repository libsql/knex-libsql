{
  "name": "@libsql/knex-libsql",
  "version": "0.1.0-pre",
  "keywords": [
    "hrana",
    "libsql",
    "sqld",
    "database",
    "knex"
  ],
  "description": "Knex dialect for libSQL",
  "repository": {
    "type": "git",
    "url": "github:libsql/knex-libsql"
  },
  "homepage": "https://github.com/libsql/knex-libsql",
  "authors": [
    "Jan Špaček <honza@turso.tech>",
    "Glauber Costa <glauber@turso.tech>"
  ],
  "license": "MIT",
  "type": "module",
  "main": "lib-cjs/index.js",
  "types": "lib-esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib-esm/index.d.ts",
      "import": "./lib-esm/index.js",
      "require": "./lib-cjs/index.js"
    }
  },
  "files": [
    "lib-cjs/**",
    "lib-esm/**"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rm -rf ./lib-cjs ./lib-esm",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.build-cjs.json",
    "build:esm": "tsc -p tsconfig.build-esm.json",
    "postbuild": "cp package-cjs.json ./lib-cjs/package.json"
  },
  "dependencies": {
    "@libsql/sqlite3": "^0.2.1",
    "sqlite3": "^5.1.6"
  },
  "peerDependencies": {
    "knex": "*"
  },
  "devDependencies": {
    "typescript": "^4.9.4"
  }
}
